# generic DHCP enabled netplan
network:
  version: 2
  ethernets:
    ens192:
      link-local: [ ipv4 ]
      dhcp4: false 
      addresses:
        - 192.168.1.33/19
      nameservers:
        addresses: [192.168.1.3, 192.168.1.4]
        search: [udp1024.com]
      routes:
        - on-link: true
          to: default
          via: 192.168.1.254
          metric: 90

