# generic DHCP enabled netplan
network:
  version: 2
  ethernets:
    ens192:
      dhcp4: false 
      dhcp6: true
      accept-ra: true
      addresses:
        - 192.168.1.41/16
      nameservers:
        addresses: [192.168.1.3, 192.168.1.4]
        search: [udp1024.com]
      routes:
        - on-link: true
          to: default
          via: 192.168.1.254
          metric: 90

