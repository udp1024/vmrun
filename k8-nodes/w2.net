# generic DHCP enabled netplan
network:
  version: 2
  ethernets:
    ens192:
      dhcp6: false
      dhcp4: false 
      addresses:
        - 192.168.1.42/20
        - 2001:56a:f85f:b900::42/64
      nameservers:
        addresses: [192.168.1.3, 192.168.1.4]
        search: [udp1024.com]
      routes:
        - on-link: true
          to: default
          via: 192.168.1.254
          metric: 90
        - on-link: true
          to: default
          via: 2001:56a:f85f:b9ff:a2b5:3cff:fe39:35d
          metric: 90
      dhcp4: false
      dhcp6: false
      accept-ra: false
      